{
  "version": 2,
  "projects": {
    "symlink-app": {
      "root": "apps/symlink-app",
      "sourceRoot": "apps/symlink-app/src",
      "projectType": "application",
      "prefix": "symlink-app",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/symlink-app",
            "main": "apps/symlink-app/src/main.ts",
            "tsConfig": "apps/symlink-app/tsconfig.app.json",
            "assets": [
              "apps/symlink-app/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/symlink-app/src/environments/environment.ts",
                  "with": "apps/symlink-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "symlink-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/symlink-app/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/symlink-app"
          ],
          "options": {
            "jestConfig": "apps/symlink-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "symlinked-lib": {
      "root": "libs/symlinked-lib",
      "sourceRoot": "libs/symlinked-lib/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/symlinked-lib/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/symlinked-lib"
          ],
          "options": {
            "jestConfig": "libs/symlinked-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "symlink-app"
}